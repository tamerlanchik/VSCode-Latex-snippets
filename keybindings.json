 [
     {
      "key": "ctrl+shift+m",
      "command": "editor.action.insertSnippet",
      "when": "editorTextFocus",
      "args": {
        "name": "Math inline mode"
      }
  },
  {
    "key": "ctrl+alt+m",
    "command": "editor.action.insertSnippet",
    "when": "editorTextFocus",
    "args": {
      "name": "Math outline mode"
    }
  },
  {
    "key": "alt+u",
    "command": "editor.action.insertSnippet",
    "when": "editorTextFocus",
    "args": {
       "command": "deleteWordLeft",
      "name": "Updashed"
    }
  },
  {
    "key": "alt+w",
    // "command": "latex-workshop.refresh-viewer",
    "command": "macros.latex.buildAndUpdate"
  }
]